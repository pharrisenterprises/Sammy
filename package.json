{
  "name": "Recorder",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.config.ts && vite build --config vite.config.bg.ts",
    "build:ex": "npm run build && copy src\\manifest.json dist\\",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postbuild": "node ./scripts/postbuild.js",
    "watch:build": "nodemon --watch src --ext js,jsx,ts,tsx,json,css --exec \"npm run build && npm run postbuild\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
